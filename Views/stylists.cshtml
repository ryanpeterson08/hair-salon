<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Stylists</title>
    <link href="/Content/css/bootstrap.css" rel="stylesheet" />
  </head>
  <body>
    <h2>All Our Stylists</h2>
    <ul>
      @foreach(var stylist in Model)
      {
        <li><a href="/stylist/@stylist.GetId()">@stylist.GetName()</li>
      }
    </ul>
    <a href="/stylists/new">Add in a stylist</a>
  </body>
</html>
